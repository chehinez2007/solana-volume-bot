{
  "name": "solana-volume-bot",
  "version": "1.0.0",
  "description": "Open source Solana volume management bot for automated market making and volume boosting",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chehinez2007/solana-volume-bot.git"
  },
  "keywords": [
    "solana",
    "trading-bot",
    "market-maker",
    "volume-booster",
    "typescript",
    "blockchain"
  ],
  "author": "chehinez2007",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chehinez2007/solana-volume-bot/issues"
  },
  "homepage": "https://github.com/chehinez2007/solana-volume-bot#readme",
  "dependencies": {
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.9.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
